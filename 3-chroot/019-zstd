# Stage1: Zstd
# This section is done in Chroot environment

# Source is hard coded for ar. Create a link

CC=clang CXX=clang++ \
    make -j`nproc --all`

CC=clang CXX=clang++ \
    make -C contrib/pzstd -j`nproc --all`

make prefix=/llvmtools \
    install
make PREFIX=/llvmtools \
    -C contrib/pzstd install
